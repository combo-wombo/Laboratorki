SELECT LASF_C.*, LASF_K.*;
 FROM LASF_K, LASF_C;
 WHERE LASF_C.LASF_C0 = LASF_K.LASF_K0;
   AND LASF_K.LASF_KDATA >= LASF_C.LASF_CDATS;
   AND LASF_K.LASF_KDATA < LASF_C.LASF_CDATD;
   AND LASF_K.LASF_KS = lasf_c.lasf_cs;
 INTO CURSOR LASF_KQO_A
REPORT FORM LASF_KRO.FRX TO FILE LASF_KfO.TXT NOCONSOLE NOEJECT

